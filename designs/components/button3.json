{
  "id": "button3_component",
  "type": "component",
  "props": {
    "name": "Button3",
    "layoutMode": "VERTICAL",
    "primaryAxisSizingMode": "AUTO",
    "counterAxisSizingMode": "AUTO",
    "paddingBottom": 20,
    "paddingLeft": 20,
    "paddingRight": 20,
    "paddingTop": 20,
    "fills": [
      {
        "type": "SOLID",
        "color": {
          "r": 1,
          "g": 1,
          "b": 0
        }
      }
    ]
  },
  "variableProps": {
    "paddingBottom": "Text Size"
  },
  "properties": {
    "state": {
      "type": "BOOLEAN",
      "name": "Enabled",
      "defaultValue": true
    },
    "label": {
      "type": "TEXT",
      "name": "Button Label",
      "defaultValue": "Click Me!"
    }
  },
  "children": [
    {
      "type": "text",
      "style": "hello world",
      "props": {
        "characters": "Button!",
        "fontSize": 25
      },
      "bindings": {
        "label": {
          "attribute": "characters"
        }
      }
    }
  ]
}
